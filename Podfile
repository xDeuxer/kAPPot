# Uncomment the next line to define a global platform for your project
 platform :ios, '9.0'

target 'kAPPot' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for kAPPot
   pod 'Firebase/Core'
   pod 'Firebase/Firestore'
   pod 'Firebase/Auth'
   pod 'FBSDKLoginKit'
   pod 'FacebookCore'
   pod 'FacebookLogin'
   pod 'GoogleMaps'
   pod 'GooglePlaces'
  target 'kAPPotTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'kAPPotUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
